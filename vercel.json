{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.allenhaydenjohnson.com"
        }
      ],
      "destination": "https://allenhaydenjohnson.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "Link",
          "value": "<https://allenhaydenjohnson.com>; rel=\"canonical\""
        }
      ]
    }
  ]
}
